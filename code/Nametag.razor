@using Sandbox;
@using Sandbox.UI;
@inherits PanelComponent

<root class="@(Network.IsProxy ? "" : "hidden")">
	<div class="title">@(Network.OwnerConnection.DisplayName)</div>
</root>

@code
{
	protected override int BuildHash() => System.HashCode.Combine( Network.IsProxy, Network.OwnerConnection.DisplayName );
}
